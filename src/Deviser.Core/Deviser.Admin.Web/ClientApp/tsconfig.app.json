{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "types": [],
    "paths": {
      "@angular/*": [
        "./node_modules/@angular/*"
      ],
      "@ng-select/*": [
        "./node_modules/@ng-select/*"
      ],
      "moment": [
        "./node_modules/moment"
      ],
      "ngx-bootstrap/*": [
        "./node_modules/ngx-bootstrap/*"
      ],
      "ngx-image-cropper": [
        "./node_modules/ngx-image-cropper"
      ],
      "ng-inline-svg": [
        "./node_modules/ng-inline-svg-2"
      ],
      "deviser-shared":[
        "../../Deviser.NgLib/deviser-workspace/dist/deviser-shared"
      ]
    }
  },
  "files": [
    "src/main.ts",
    "src/polyfills.ts"
  ],
  "include": [
    "src/**/*.d.ts"
  ]
}
